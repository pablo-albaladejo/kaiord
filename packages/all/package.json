{
  "name": "@kaiord/all",
  "version": "2.0.0",
  "description": "Meta-package that includes all Kaiord format adapters for backward compatibility",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest --run",
    "test:watch": "vitest",
    "lint": "tsc --noEmit && eslint . && prettier --check src",
    "lint:fix": "eslint . --fix && prettier --write src",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@kaiord/core": "workspace:^",
    "@kaiord/fit": "workspace:^",
    "@kaiord/tcx": "workspace:^",
    "@kaiord/zwo": "workspace:^"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^8.5.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pablo-albaladejo/kaiord.git",
    "directory": "packages/all"
  },
  "homepage": "https://www.npmjs.com/package/@kaiord/all",
  "bugs": {
    "url": "https://github.com/pablo-albaladejo/kaiord/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "kaiord",
    "fit",
    "tcx",
    "zwo",
    "workout",
    "converter",
    "fitness"
  ],
  "author": "Kaiord Contributors",
  "license": "MIT"
}
