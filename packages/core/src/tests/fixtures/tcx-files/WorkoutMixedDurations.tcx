<?xml version="1.0" encoding="UTF-8"?>
<TrainingCenterDatabase xmlns="http://www.garmin.com/xmlschemas/TrainingCenterDatabase/v2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.garmin.com/xmlschemas/TrainingCenterDatabase/v2 http://www.garmin.com/xmlschemas/TrainingCenterDatabasev2.xsd">
  <Workouts>
    <Workout Sport="Running">
      <Name>Mixed Duration</Name>
      <Step xsi:type="Step_t">
        <StepId>1</StepId>
        <Name>Time Based</Name>
        <Duration xsi:type="Time_t">
          <Seconds>300</Seconds>
        </Duration>
        <Intensity>Active</Intensity>
        <Target xsi:type="HeartRate_t">
          <HeartRateZone xsi:type="PredefinedHeartRateZone_t">
            <Number>2</Number>
          </HeartRateZone>
        </Target>
      </Step>
      <Step xsi:type="Step_t">
        <StepId>2</StepId>
        <Name>Distance Based</Name>
        <Duration xsi:type="Distance_t">
          <Meters>1000</Meters>
        </Duration>
        <Intensity>Active</Intensity>
        <Target xsi:type="Speed_t">
          <SpeedZone xsi:type="CustomSpeedZone_t">
            <ViewAs>Pace</ViewAs>
            <LowInMetersPerSecond>3.0</LowInMetersPerSecond>
            <HighInMetersPerSecond>3.5</HighInMetersPerSecond>
          </SpeedZone>
        </Target>
      </Step>
      <Step xsi:type="Step_t">
        <StepId>3</StepId>
        <Name>Lap Button</Name>
        <Duration xsi:type="UserInitiated_t"/>
        <Intensity>Active</Intensity>
        <Target xsi:type="Cadence_t">
          <Low>80</Low>
          <High>90</High>
        </Target>
      </Step>
      <Step xsi:type="Step_t">
        <StepId>4</StepId>
        <Name>Calorie Based</Name>
        <Duration xsi:type="CaloriesBurned_t">
          <Calories>100</Calories>
        </Duration>
        <Intensity>Active</Intensity>
        <Target xsi:type="None_t"/>
      </Step>
      <Step xsi:type="Step_t">
        <StepId>5</StepId>
        <Name>HR Above</Name>
        <Duration xsi:type="HeartRateAbove_t">
          <HeartRate xsi:type="HeartRateInBeatsPerMinute_t">
            <Value>160</Value>
          </HeartRate>
        </Duration>
        <Intensity>Active</Intensity>
        <Target xsi:type="None_t"/>
      </Step>
      <Step xsi:type="Step_t">
        <StepId>6</StepId>
        <Name>HR Below</Name>
        <Duration xsi:type="HeartRateBelow_t">
          <HeartRate xsi:type="HeartRateInBeatsPerMinute_t">
            <Value>120</Value>
          </HeartRate>
        </Duration>
        <Intensity>Resting</Intensity>
        <Target xsi:type="None_t"/>
      </Step>
    </Workout>
  </Workouts>
</TrainingCenterDatabase>
