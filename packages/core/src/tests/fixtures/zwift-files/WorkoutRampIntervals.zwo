<?xml version="1.0" encoding="UTF-8"?>
<workout_file>
  <author>Kaiord Test Suite</author>
  <name>Ramp Intervals Workout</name>
  <description>Workout with progressive power ramps for testing Warmup, Ramp, and Cooldown conversion</description>
  <sportType>bike</sportType>
  <tags>
    <tag>test</tag>
    <tag>ramp</tag>
  </tags>
  <workout>
    <Warmup Duration="600" PowerLow="0.45" PowerHigh="0.65" Cadence="85"/>
    <SteadyState Duration="300" Power="0.75" Cadence="90"/>
    <Ramp Duration="480" PowerLow="0.75" PowerHigh="1.05" Cadence="95"/>
    <SteadyState Duration="300" Power="1.05" Cadence="95"/>
    <Ramp Duration="300" PowerLow="1.05" PowerHigh="0.85" Cadence="90"/>
    <Cooldown Duration="600" PowerLow="0.65" PowerHigh="0.45" Cadence="80"/>
  </workout>
</workout_file>
