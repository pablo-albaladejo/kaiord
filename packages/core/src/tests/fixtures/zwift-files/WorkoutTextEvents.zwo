<?xml version="1.0" encoding="UTF-8"?>
<workout_file>
  <author>Kaiord Test Suite</author>
  <name>Text Events Workout</name>
  <description>Workout demonstrating text events (coaching cues) with time and distance offsets</description>
  <sportType>bike</sportType>
  <tags>
    <tag>test</tag>
    <tag>text-events</tag>
  </tags>
  <workout>
    <SteadyState Duration="300" Power="0.60" Cadence="85">
      <textevent message="Easy warmup, find your rhythm" timeoffset="0"/>
      <textevent message="Increase cadence slightly" timeoffset="120"/>
    </SteadyState>
    <IntervalsT Repeat="3" OnDuration="120" OffDuration="60" OnPower="1.05" OffPower="0.55" Cadence="95" CadenceResting="80">
      <textevent message="Push hard for 2 minutes!" timeoffset="0"/>
      <textevent message="Halfway through the interval" timeoffset="60"/>
      <textevent message="Recovery - spin easy" timeoffset="120"/>
    </IntervalsT>
    <Ramp Duration="480" PowerLow="0.70" PowerHigh="1.00" Cadence="90">
      <textevent message="Gradual build to threshold" timeoffset="0"/>
      <textevent message="Keep cadence steady" timeoffset="180"/>
      <textevent message="Almost at threshold power" timeoffset="360"/>
    </Ramp>
    <SteadyState Duration="600" Power="1.00" Cadence="95">
      <textevent message="Hold threshold for 10 minutes" timeoffset="0"/>
      <textevent message="5 minutes down, 5 to go" timeoffset="300"/>
      <textevent message="Final minute - stay strong!" timeoffset="540"/>
    </SteadyState>
    <Cooldown Duration="600" PowerLow="0.60" PowerHigh="0.40" Cadence="75">
      <textevent message="Easy spin to cool down" timeoffset="0"/>
      <textevent message="Great work today!" timeoffset="300"/>
    </Cooldown>
  </workout>
</workout_file>
