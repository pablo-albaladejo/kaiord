<?xml version="1.0" encoding="UTF-8"?>
<workout_file>
  <author>Kaiord Test Suite</author>
  <name>Mixed Interval Types Workout</name>
  <description>Complex workout combining all interval types for comprehensive testing</description>
  <sportType>bike</sportType>
  <tags>
    <tag>test</tag>
    <tag>mixed</tag>
    <tag>comprehensive</tag>
  </tags>
  <workout>
    <Warmup Duration="600" PowerLow="0.45" PowerHigh="0.65" Cadence="85"/>
    <SteadyState Duration="300" Power="0.75" Cadence="90"/>
    <IntervalsT Repeat="4" OnDuration="90" OffDuration="60" OnPower="1.10" OffPower="0.55" Cadence="100" CadenceResting="80"/>
    <FreeRide Duration="180" Cadence="85" FlatRoad="1"/>
    <Ramp Duration="300" PowerLow="0.70" PowerHigh="0.95" Cadence="90"/>
    <SteadyState Duration="600" Power="0.95" Cadence="95"/>
    <IntervalsT Repeat="3" OnDuration="120" OffDuration="90" OnPower="1.05" OffPower="0.60" Cadence="95" CadenceResting="85"/>
    <Cooldown Duration="600" PowerLow="0.60" PowerHigh="0.40" Cadence="75"/>
  </workout>
</workout_file>
