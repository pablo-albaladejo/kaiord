<?xml version="1.0" encoding="UTF-8"?>
<workout_file>
  <author>Kaiord Test Suite</author>
  <name>Interval Training Workout</name>
  <description>Workout with IntervalsT blocks for testing repeated interval conversion</description>
  <sportType>bike</sportType>
  <tags>
    <tag>test</tag>
    <tag>intervals</tag>
  </tags>
  <workout>
    <SteadyState Duration="300" Power="0.55" Cadence="85"/>
    <IntervalsT Repeat="5" OnDuration="120" OffDuration="60" OnPower="1.05" OffPower="0.50" Cadence="95" CadenceResting="75"/>
    <IntervalsT Repeat="3" OnDuration="180" OffDuration="90" OnPower="0.95" OffPower="0.55" Cadence="90" CadenceResting="80"/>
    <SteadyState Duration="300" Power="0.50" Cadence="80"/>
  </workout>
</workout_file>
