{
  "name": "@kaiord/zwo",
  "version": "4.0.0",
  "description": "ZWO format adapter for Kaiord workout data conversion",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "browser": {
        "types": "./dist/browser.d.ts",
        "import": "./dist/browser.js"
      },
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "schema"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest --run",
    "test:watch": "vitest",
    "lint": "tsc --noEmit && eslint . && prettier --check src",
    "lint:fix": "eslint . --fix && prettier --write src",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@kaiord/core": "workspace:^",
    "fast-xml-parser": "^5.3.4",
    "xsd-schema-validator": "^0.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^24.0.0",
    "@vitest/coverage-v8": "^4.0.18",
    "tsup": "^8.5.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pablo-albaladejo/kaiord.git",
    "directory": "packages/zwo"
  },
  "homepage": "https://www.npmjs.com/package/@kaiord/zwo",
  "bugs": {
    "url": "https://github.com/pablo-albaladejo/kaiord/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "kaiord",
    "zwo",
    "zwift",
    "workout",
    "converter",
    "fitness"
  ],
  "author": "Kaiord Contributors",
  "license": "MIT"
}
